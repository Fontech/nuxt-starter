{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint 'assets/**/*.{s?(a|c)ss,less,stylus}' '{components,layouts,pages}/**/*.vue'",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    <%_ if (ui === 'bulma') { _%>
    "@nuxtjs/bulma": "^1.2.1",
    <%_ } else if (ui === 'ant-design-vue') { _%>
    "ant-design-vue": "^1.1.10",
    <%_ } else if (ui === 'bootstrap') { _%>
    "bootstrap-vue": "^2.0.0",
    "bootstrap": "^4.1.3",
    <%_ } else if (ui === 'element-ui') { _%>
    "element-ui": "^2.4.11",
    <%_ } else if (ui === 'framevuerk') { _%>
    "framevuerk": "^2.2.5",
    <%_ } else if (ui === 'iview') { _%>
    "iview": "^3.1.5",
    <%_ } else if (ui === 'buefy') { _%>
    "nuxt-buefy": "^0.3.2",
    <%_ } else if (ui === 'tachyons') { _%>
    "tachyons": "^4.11.1",
    <%_ } _%>
    <%_ if (edge) { _%>
    "nuxt-edge": "latest"
    <%_ } else { _%>
    "nuxt": "^2.0.0"
    <%_ } _%>
  },
  "devDependencies": {
    <%_ if (ui === 'tailwind') { _%>
    "@nuxtjs/tailwindcss": "^1.0.0",
    <%_ } else if (ui === 'vuetify') { _%>
    "@nuxtjs/vuetify": "^1.0.0",
    <%_ } else if (ui === 'framevuerk') { _%>
    "framevuerk-builder": "^2.0.2",
    <%_ } _%>
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.16.4",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^4.0.0-0"
  }
}
